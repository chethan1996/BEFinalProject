        "def main():\n",
        "            MY_ADDRESS = 'securesystem57@gmail.com'\n",
        "            PASSWORD = 'securesystem2020'\n",
        "            names, emails = get_contacts('contacts.txt') # read contacts\n",
        "            message_template = read_template('msg.txt')\n",
        "\n",
        "            # set up the SMTP server\n",
